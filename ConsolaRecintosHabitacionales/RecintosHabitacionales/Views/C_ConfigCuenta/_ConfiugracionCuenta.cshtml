@model DTOs.ConfiguracionCuenta.ConfiguraCuentasDTOCompleto
@using Utilitarios;

@{
	Layout = "";
}

  <form asp-action="GestionarConfiguracionCuenta" id="GestionarConfiguracionCuenta">
<div class="card card-flush">
    <div class="card card-body">
            <div class="row">
                @Html.HiddenFor(x => x.IdConfiguracionCuenta)
                @Html.HiddenFor(x => x.IdConjunto)
                @Html.HiddenFor(x => x.FechaCreacion)
                @Html.HiddenFor(x => x.UsuarioCreacion)
                <input asp-for="Parametrizacion" placeholder="1.1.00.00.00" class="form-control form-control-solid" />
                </div>
                <div class="row">
                    <div class="col-xl-12 fv-row">
                        <center>
                            <button type="button" onclick="sendFormAjax(this, 'GestionarConfiguracionCuenta', undefined,undefined,'btn_Configuracion')" class="btn btn-primary" id="btn_Configuracion">
                                <span class="indicator-label">
                                    <i class="far fa-save fs-md-2qx"></i>
                                    Guardar
                                    </span>
                                    <span class="indicator-progress">
                                        Por favor espere... <span class="spinner-border spinner-border-sm align-middle ms-2"></span>
                                        </span>
                            </button>
                         </center>
                </div>
            </div>
    </div>
</div>

    
</form>

